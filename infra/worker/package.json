{
  "name": "pname",
  "version": "1.0.0",
  "description": "Cloudflare worker TypeScript template",
  "license": "MIT OR Apache-2.0",
  "author": "author",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "cloudworker": "cross-env IN_CLOUDWORKER=true cloudworker -d -p 8787 --kv-set PUBLIC_KEYS.key=value --kv-set REFRESH_TOKENS.key=value  --watch \"./dist/worker.dev.js\"",
    "stop-dev": "kill$(lsof -ti:8787) ; rm dist/worker.js",
    "build:dev": "cross-env NODE_ENV=development webpack",
    "build:watch": "nodemon --watch src --exec \"npm run build:dev && mv dist/worker.js dist/worker.dev.js\" -e ts",
    "dev": "npm run stop-dev ; concurrently \"npm:build:watch\" \"wait-on dist/worker.dev.js && npm run cloudworker\"",
    "test": "jest --config jestconfig.json --coverage",
    "lint:fix": "eslint --fix --ext .ts,.tsx .",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.45.0",
    "@aws-sdk/s3-request-presigner": "^3.45.0",
    "@cloudflare/kv-asset-handler": "^0.1.1",
    "aws4fetch": "^1.0.13",
    "encrypt-workers-kv": "^0.0.3",
    "itty-router": "^2.3.10",
    "itty-router-extras": "^0.2.3",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^2.0.0",
    "@peculiar/webcrypto": "^1.1.7",
    "@types/atob": "^2.1.2",
    "@types/btoa": "^1.2.3",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^14.14.35",
    "@types/service-worker-mock": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "atob": "^2.1.2",
    "btoa": "^1.2.1",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.22.0",
    "jest": "^27.0.1",
    "jest-fetch-mock": "^3.0.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "service-worker-mock": "^2.0.5",
    "source-map-support": "^0.5.19",
    "ts-jest": "^27.0.1",
    "ts-loader": "^7.0.5",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.2",
    "wait-on": "^5.3.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12"
  }
}
{
  "name": "pname",
  "version": "1.0.0",
  "description": "Cloudflare worker TypeScript template",
  "main": "index.js",
  "scripts": {
    "build": "webpack --display none",
    "cloudworker": "IN_CLOUDWORKER=true cloudworker -d -p 8787 --kv-set PUBLIC_KEYS.key=value --kv-set REFRESH_TOKENS.key=value --kv-set ONLINE_USERS.key=value --watch \"./dist/worker.dev.js\"",
    "stop-dev": "kill$(lsof -ti:8787) ; rm dist/worker.js",
    "build:dev": "NODE_ENV=development webpack --display none",
    "build:watch": "nodemon --watch src --exec \"npm run build:dev && mv dist/worker.js dist/worker.dev.js\" -e ts",
    "dev": "npm run stop-dev ; concurrently \"npm:build:watch\" \"wait-on dist/worker.dev.js && npm run cloudworker\"",
    "deploy": "wrangler publish",
    "test:clean": "rimraf ./transpiled/src ./transpiled/test",
    "test": "npm run test:clean && npm run transpile && mocha --require source-map-support/register --recursive transpiled/test",
    "transpile": "tsc --project ./test",
    "lint:fix": "eslint --fix --ext .ts,.tsx ."
  },
  "author": "author",
  "license": "MIT OR Apache-2.0",
  "devDependencies": {
    "@cloudflare/workers-types": "^2.0.0",
    "@peculiar/webcrypto": "^1.1.7",
    "@types/chai": "^4.2.11",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.14.35",
    "@types/sinon": "^10.0.2",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "chai": "^4.2.0",
    "concurrently": "^6.0.0",
    "eslint": "^7.22.0",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "service-worker-mock": "^2.0.5",
    "sinon": "^11.1.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "wait-on": "^5.3.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.1.1",
    "encrypt-workers-kv": "^0.0.3",
    "itty-router": "^2.3.10",
    "itty-router-extras": "^0.2.3",
    "jsonwebtoken": "^8.5.1"
  }
}
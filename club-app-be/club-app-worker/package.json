{
  "name": "pname",
  "version": "1.0.0",
  "description": "Cloudflare worker TypeScript template",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=development webpack --display none",
    "stop-dev": "kill $(lsof -ti:8787)",
    "dev": "nodemon --watch src --exec \"npm run stop-dev ; npm run build && cloudworker -d -p 8787 --kv-set PUBLIC_KEYS.key=value --kv-set REFRESH_TOKENS.key=value ./dist/worker.js\" -e ts",
    "deploy": "wrangler publish",
    "test:clean": "rimraf ./transpiled/src ./transpiled/test",
    "test": "npm run test:clean && npm run transpile && mocha --require source-map-support/register --recursive transpiled/test",
    "transpile": "tsc --project ./test",
    "lint:fix": "eslint --fix --ext .ts,.tsx ."
  },
  "author": "author",
  "license": "MIT OR Apache-2.0",
  "devDependencies": {
    "@cloudflare/workers-types": "^2.0.0",
    "@types/chai": "^4.2.11",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.14.35",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "chai": "^4.2.0",
    "eslint": "^7.22.0",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "service-worker-mock": "^2.0.5",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.1.1",
    "concurrently": "^6.0.0",
    "encrypt-workers-kv": "^0.0.3",
    "itty-router": "^2.1.9",
    "itty-router-extras": "^0.2.3",
    "jsonwebtoken": "^8.5.1"
  }
}

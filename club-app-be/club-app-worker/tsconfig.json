{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "commonjs",
    "target": "es2018",
    "lib": [
      "es2018",
      "webworker"
    ],
    "alwaysStrict": true,
    "incremental": true,
    "strict": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "types": [
      "@cloudflare/workers-types",
      "@types/jest",
      "@types/service-worker-mock",
    ]
  },
  "files": [
    "decs.d.ts"
  ],
  "include": [
    "src",
    "test"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}